emove: (key) => {\n\t\t// return CrudService.removeData('surat-keluar',key);\n        return axios.delete(API_URL +'surat-keluar'+\"/\"+key).then(function(response) {\n\t\t\tToast.fire({\n\t\t\t\ticon: response.data.status,\n\t\t\t\ttitle: response.data.message\n\t\t\t})\n\t\t}).catch((response) => {\n\t\t\tToast.fire({\n\t\t\t\ticon: response.data.status,\n\t\t\t\ttitle: response.data.message\n\t\t\t})\n\t\t})\n\t},\n\tupdate: (key, values) => {\n\t\t// return CrudService.updateData('surat-keluar',key,values);\n        return axios.put(API_URL +'surat-keluar'+\"/\"+key,values).then(function(response){\n\t\t\tToast.fire({\n\t\t\t\ticon: response.data.status,\n\t\t\t\ttitle: response.data.message\n\t\t\t})\n\t\t})\n\t\t.catch((response)=>{\n\t\t\tToast.fire({\n\t\t\t\ticon: response.data.status,\n\t\t\t\ttitle: response.data.message\n\t\t\t})\n\t\t})\n\t}\n});\n\nexport default {\n  name: 'SuratKeluar',\n  components: {\n    IonButtons,\n    IonContent,\n    IonHeader,\n    IonMenuButton,\n    IonPage,\n    IonTitle,\n    IonToolbar,\n    DxDataGrid,\n    DxColumn,\n    DxPaging,\n    DxEditing,\n    DxPopup,\n    DxScrolling\n  },\n  data() {\n    return {\n      dataSource: store,\n    };\n  },\n  methods: {\n    statusColumn(e) {\n        const stext = [\"Draft\",\"Waiting Approval\",\"Approved\",\"Rejected\",\"\"];\n\n        return stext[e.value];\n    }\n  },\n}\n</script>\n\n<style scoped>\n#container {\n  text-align: center;\n  position: absolute;\n  left: 0;\n  right: 0;\n  top: 50%;\n  transform: translateY(-50%);\n}\n\n#container strong {\n  font-size: 20px;\n  line-height: 26px;\n}\n\n#container p {\n  font-size: 16px;\n  line-height: 22px;\n  color: #8c8c8c;\n  margin: 0;\n}\n\n#container a {\n  text-decoration: none;\n}\n</style>"]}]}