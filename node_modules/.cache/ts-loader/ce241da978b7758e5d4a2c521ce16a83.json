ustom_store';\nimport { DxItem } from 'devextreme-vue/form';\nimport 'devextreme/dist/css/dx.material.teal.light.compact.css';\n// import CrudService from '../service/crud.service.js';\nimport axios from 'axios';\nimport Swal from 'sweetalert2';\n\nconst Toast = Swal.mixin({\n  toast: true,\n  position: 'top-end',\n  showConfirmButton: false,\n  timer: 3000,\n  timerProgressBar: true,\n  didOpen: (toast) => {\n    toast.addEventListener('mouseenter', Swal.stopTimer)\n    toast.addEventListener('mouseleave', Swal.resumeTimer)\n  }\n})\n// const API_URL = 'http://127.0.0.1:8000/api/';\nconst API_URL = 'http://esurat-um.com/api/';\n\n\n\nconst store = new CustomStore({\n\tkey: 'id_surat_masuk',\n\tload: function() {\n\t\t// return CrudService.getAll('surat-masuk');\n        return axios.get(API_URL + 'surat-masuk').then(response => response.data);\n\t},\n\t// insert: (values) => {\n\t// \treturn CrudService.insertData('surat-masuk',values);\n\t// },\n\t// remove: (key) => {\n\t// \treturn CrudService.removeData('surat-masuk',key);\n\t// },\n\tupdate: (key, values) => {\n\t\t// return CrudService.updateData('surat-masuk',key,values);\n        return axios.put(API_URL +'surat-masuk'+\"/\"+key,values).then(()=>{\n\t\t\tToast.fire({\n\t\t\t\ticon: 'success',\n\t\t\t\ttitle: 'Data updated successfully'\n\t\t\t})\n\t\t})\n\t\t.catch(()=>{\n\t\t\tSwal.fire({\n\t\t\t\ticon: 'error',\n\t\t\t\ttitle: 'Oops...',\n\t\t\t\ttext: 'Something went wrong!',\n\t\t\t\tfooter: '<a href>Why do I have this issue?</a>'\n\t\t\t})\n\t\t})\n\t}\n});\n\nexport default {\n  name: 'SuratMasuk',\n  components: {\n    IonButtons,\n    IonContent,\n    IonHeader,\n    IonMenuButton,\n    IonPage,\n    IonTitle,\n    IonToolbar,\n    DxDataGrid,\n    DxColumn,\n    DxPaging,\n    DxEditing,\n    DxPopup,\n    // DxLookup,\n    DxForm,\n    DxItem,\n    DxScrolling\n  },\n  data() {\n    return {\n      dataSource: store,\n    //   states: states\n      // columns: ['CompanyName', 'City', 'State', 'Phone', 'Fax']\n    };\n  }\n}\n"],"sourceRoot":""}]}