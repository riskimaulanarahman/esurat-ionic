ption=\"Home Address\"\n                >\n                    <DxItem data-field=\"StateID\"/>\n                    <DxItem data-field=\"Address\"/>\n                </DxItem> -->\n                </DxForm>\n            </DxEditing>\n\n            <!-- <DxColumn\n                :width=\"70\"\n                data-field=\"Prefix\"\n                caption=\"Title\"\n            /> -->\n            <DxColumn data-field=\"no_surat\"/>\n            <DxColumn data-field=\"perihal_surat\"/>\n            <DxColumn data-field=\"tgl_surat\"/>\n            <DxColumn data-field=\"tgl_diterima\"/>\n            <DxColumn data-field=\"asal_surat\"/>\n            <DxColumn data-field=\"alamat\"/>\n            <DxColumn data-field=\"kontak_person\"/>\n            <DxColumn data-field=\"sifat_surat\"/>\n            <DxColumn data-field=\"tindak_lanjut\"/>\n            <!-- <DxColumn data-field=\"Disposisi\"/> -->\n            <DxColumn data-field=\"status\"/>\n            <DxColumn data-field=\"file_surat_masuk\"/>\n            <!-- <DxColumn data-field=\"Disposisi\"/> -->\n            <!-- <DxColumn data-field=\"LastName\"/> -->\n            <!-- <DxColumn\n                data-field=\"BirthDate\"\n                data-type=\"date\"\n            />\n            <DxColumn\n                :width=\"170\"\n                data-field=\"Position\"\n            />\n            <DxColumn\n                data-field=\"HireDate\"\n                data-type=\"date\"\n            />\n            <DxColumn\n                :width=\"125\"\n                data-field=\"StateID\"\n                caption=\"State\"\n            >\n                <DxLookup\n                :data-source=\"states\"\n                value-expr=\"ID\"\n                display-expr=\"Name\"\n                />\n            </DxColumn>\n            <DxColumn\n                :visible=\"false\"\n                data-field=\"Address\"\n            />\n            <DxColumn\n                :visible=\"false\"\n          