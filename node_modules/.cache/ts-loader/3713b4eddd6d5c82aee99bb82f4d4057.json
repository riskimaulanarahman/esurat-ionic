-grid';\nimport CustomStore from 'devextreme/data/custom_store';\nimport { DxItem } from 'devextreme-vue/form';\nimport 'devextreme/dist/css/dx.material.teal.light.compact.css';\nimport CrudService from './crudservice';\nimport Modal from './Modal';\n\nconst store = new CustomStore({\n\tkey: 'id_surat_masuk',\n\tload: function() {\n\t\treturn CrudService.getAll('surat-masuk');\n\t},\n\tinsert: (values) => {\n\t\treturn CrudService.insertData('surat-masuk',values);\n\t},\n\tremove: (key) => {\n\t\treturn CrudService.removeData('surat-masuk',key);\n\t},\n\tupdate: (key, values) => {\n\t\treturn CrudService.updateData('surat-masuk',key,values);\n\t}\n});\n\nexport default {\n  name: 'SuratMasuk',\n  components: {\n    IonButtons,\n    IonContent,\n    IonHeader,\n    IonMenuButton,\n    IonPage,\n    IonTitle,\n    IonToolbar,\n    DxDataGrid,\n    DxColumn,\n    DxPaging,\n    DxEditing,\n    DxPopup,\n    // DxLookup,\n    // DxForm,\n    // DxItem,\n    DxScrolling\n  },\n  methods: {\n    statustext(options) {\n        const stext = [\"Draft\",\"Waiting Approval\",\"Approved\",\"Rejected\",\"\"];\n\n        return stext[options.value];\n    },\n    async openModal() {\n      const modal = await modalController\n        .create({\n          component: Modal,\n          cssClass: 'my-custom-class',\n          componentProps: {\n            title: 'Upload Berkas'\n          },\n          swipeToClose: true,\n        })\n      return modal.present();\n    },\n  },\n  data() {\n    return {\n      dataSource: store,\n    };\n  },\n}\n"],"sourceRoot":""}]}