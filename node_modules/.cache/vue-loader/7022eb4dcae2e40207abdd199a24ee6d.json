ns=\"{ height: 100 }\"\n                    data-field=\"Notes\"\n                    editor-type=\"dxTextArea\"\n                    /> -->\n                <!-- </DxItem> -->\n\n                <!-- <DxItem\n                    :col-count=\"2\"\n                    :col-span=\"2\"\n                    item-type=\"group\"\n                    caption=\"Home Address\"\n                >\n                    <DxItem data-field=\"StateID\"/>\n                    <DxItem data-field=\"Address\"/>\n                </DxItem> -->\n                </DxForm>\n            </DxEditing>\n\n            <!-- <DxColumn\n                :width=\"70\"\n                data-field=\"Prefix\"\n                caption=\"Title\"\n            /> -->\n            <DxColumn data-field=\"no_surat\"/>\n            <DxColumn data-field=\"perihal_surat\"/>\n            <DxColumn data-field=\"tgl_surat\"/>\n            <DxColumn data-field=\"tgl_diterima\"/>\n            <DxColumn data-field=\"asal_surat\"/>\n            <DxColumn data-field=\"alamat\"/>\n            <DxColumn data-field=\"kontak_person\"/>\n            <DxColumn data-field=\"sifat_surat\"/>\n            <DxColumn data-field=\"tindak_lanjut\"/>\n            <!-- <DxColumn data-field=\"Disposisi\"/> -->\n            <DxColumn data-field=\"status\"/>\n            <DxColumn data-field=\"file_surat_masuk\"/>\n            <!-- <DxColumn data-field=\"Disposisi\"/> -->\n            <!-- <DxColumn data-field=\"LastName\"/> -->\n            <!-- <DxColumn\n                data-field=\"BirthDate\"\n                data-type=\"date\"\n            />\n            <DxColumn\n                :width=\"170\"\n                data-field=\"Position\"\n            />\n            <DxColumn\n                data-field=\"HireDate\"\n                data-type=\"date\"\n            />\n            <DxColumn\n                :width=\"125\"\n                data-field=\"StateID\"\n                caption=\"State\"\n            >\n                <DxLookup\n                :data-source=\"states\"\n                value-expr=\"ID\"\n                display-expr=\"Name\"\n                />\n            </DxColumn>\n            <DxColumn\n                :visible=\"false\"\n                data-field=\"Address\"\n            />\n            <DxColumn\n                :visible=\"false\"\n                data-field=\"Notes\"\n            /> -->\n            <DxScrolling column-rendering-mode=\"virtual\"/>\n      </DxDataGrid>\n      <!-- </div> -->\n    </ion-content>\n  </ion-page>\n</template>\n\n<script lang=\"ts\">\nimport { IonButtons, IonContent, IonHeader, IonMenuButton, IonPage, IonTitle, IonToolbar } from '@ionic/vue';\nimport {\n  DxDataGrid,\n  DxColumn,\n  DxPaging,\n  DxEditing,\n  DxPopup,\n//   DxLookup,\n  DxForm,\n  DxScrolling\n} from 'devextreme-vue/data-grid';\nimport CustomStore from 'devextreme/data/custom_store';\nimport { DxItem } from 'devextreme-vue/form';\nimport 'devextreme/dist/css/dx.material.teal.light.compact.css';\n// import CrudService from '../service/crud.service.js';\nimport axios from 'axios';\nimport Swal from 'sweetalert2';\n\nconst Toast = Swal.mixin({\n  toast: true,\n  position: 'top-end',\n  showConfirmButton: false,\n  timer: 3000,\n  timerProgressBar: true,\n  didOpen: (toast) => {\n    toast.addEventListener('mouseenter', Swal.stopTimer)\n    toast.addEventListener('mouseleave', Swal.resumeTimer)\n  }\n})\n// const API_URL = 'http://127.0.0.1:8000/api/';\nconst API_URL = 'http://esurat-um.com/api/';\n\n\n\nconst store = new CustomStore({\n\tkey: 'id_surat_masuk',\n\tload: function() {\n\t\t// return CrudService.getAll('surat-masuk');\n        return axios.get(API_URL + 'surat-masuk').then(response => response.data);\n\t},\n\t// insert: (values) => {\n\t// \treturn CrudService.insertData('surat-masuk',values);\n\t// },\n\t// remove: (key) => {\n\t// \treturn CrudService.removeData('surat-masuk',key);\n\t// },\n\tupdate: (key, values) => {\n\t\t// return CrudService.updateData('surat-masuk',key,values);\n        return axios.put(API_URL +'surat-masuk'+\"/\"+key,values).then(()=>{\n\t\t\tToast.fire({\n\t\t\t\ticon: 'success',\n\t\t\t\ttitle: 'Data updated successfully'\n\t\t\t})\n\t\t})\n\t\t.catch(()=>{\n\t\t\tSwal.fire({\n\t\t\t\ticon: 'error',\n\t\t\t\ttitle: 'Oops...',\n\t\t\t\ttext: 'Something went wrong!',\n\t\t\t\tfooter: '<a href>Why do I have this issue?</a>'\n\t\t\t})\n\t\t})\n\t}\n});\n\nexport default {\n  name: 'SuratMasuk',\n  components: {\n    IonButtons,\n    IonContent,\n    IonHeader,\n    IonMenuButton,\n    IonPage,\n    IonTitle,\n    IonToolbar,\n    DxDataGrid,\n    DxColumn,\n    DxPaging,\n    DxEditing,\n    DxPopup,\n    // DxLookup,\n    DxForm,\n    DxItem,\n    DxScrolling\n  },\n  data() {\n    return {\n      dataSource: store,\n    //   states: states\n      // columns: ['CompanyName', 'City', 'State', 'Phone', 'Fax']\n    };\n  }\n}\n</script>\n\n<style scoped>\n#container {\n  text-align: center;\n  position: absolute;\n  left: 0;\n  right: 0;\n  top: 50%;\n  transform: translateY(-50%);\n}\n\n#container strong {\n  font-size: 20px;\n  line-height: 26px;\n}\n\n#container p {\n  font-size: 16px;\n  line-height: 22px;\n  color: #8c8c8c;\n  margin: 0;\n}\n\n#container a {\n  text-decoration: none;\n}\n</style>"]}]}