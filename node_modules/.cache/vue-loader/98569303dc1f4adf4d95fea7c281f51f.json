E,CAAC,CAAC,CAAC,CAAC;EACf,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;AACnB;;AAEA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,EAAE;EACX,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;EACf,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;EACjB,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;EACd,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC;AACX;;AAEA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,EAAE;EACX,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;AACvB","file":"/Users/riskimaulanarahman/Downloads/UM/esurat/android/ionic-vue-menu/src/views/SuratMasuk.vue","sourceRoot":"","sourcesContent":["<template>\n  <ion-page>\n    <ion-header :translucent=\"true\">\n      <ion-toolbar>\n        <ion-buttons slot=\"start\">\n          <ion-menu-button color=\"primary\"></ion-menu-button>\n        </ion-buttons>\n        <ion-title>Surat Masuk</ion-title>\n      </ion-toolbar>\n    </ion-header>\n    \n    <ion-content :fullscreen=\"true\">\n    \n      <!-- <div id=\"container\"> -->\n          <DxDataGrid\n            :data-source=\"dataSource\"\n            :show-borders=\"true\"\n            key-expr=\"id_surat_masuk\"\n            :column-hiding-enabled=\"true\"\n            >\n            <DxPaging :enabled=\"false\"/>\n            <DxEditing\n                :allow-updating=\"true\"\n                :allow-adding=\"true\"\n                :allow-deleting=\"true\"\n                mode=\"popup\"\n            >\n                <DxPopup\n                :show-title=\"true\"\n                :width=\"700\"\n                :height=\"525\"\n                title=\"Surat Masuk\"\n                />\n            </DxEditing>\n\n            <DxColumn \n            caption=\"upload\"\n            cell-template=\"grid-cell\"\n            />\n            <template #grid-cell>\n                <ion-button @click=\"openModal\" size=\"small\" color=\"danger\">Upload</ion-button>\n            </template>\n            <DxColumn data-field=\"no_surat\"/>\n            <DxColumn data-field=\"perihal_surat\"/>\n            <DxColumn data-field=\"tgl_surat\"/>\n            <DxColumn data-field=\"tgl_diterima\"/>\n            <DxColumn data-field=\"asal_surat\"/>\n            <DxColumn data-field=\"alamat\"/>\n            <DxColumn data-field=\"kontak_person\"/>\n            <DxColumn data-field=\"sifat_surat\"/>\n            <DxColumn \n            data-field=\"status\"\n            :encode-html=\"false\"\n            :form-item=\"{visible:false}\"\n            :customize-text=\"statusColumn_customizeText\"\n            />\n            <DxColumn \n            :form-item=\"{visible:false}\"\n            data-field=\"file_surat_masuk\"/>\n          \n            <DxScrolling column-rendering-mode=\"virtual\"/>\n      </DxDataGrid>\n      <!-- </div> -->\n    </ion-content>\n  </ion-page>\n</template>\n\n\n<script lang=\"ts\">\nimport { IonButtons, IonContent, IonHeader, IonMenuButton, IonPage, IonTitle, IonToolbar, modalController } from '@ionic/vue';\nimport {\n  DxDataGrid,\n  DxColumn,\n  DxPaging,\n  DxEditing,\n  DxPopup,\n//   DxLookup,\n  DxForm,\n  DxScrolling\n} from 'devextreme-vue/data-grid';\nimport CustomStore from 'devextreme/data/custom_store';\nimport { DxItem } from 'devextreme-vue/form';\nimport 'devextreme/dist/css/dx.material.teal.light.compact.css';\nimport CrudService from './crudservice';\nimport Modal from './Modal';\n\nconst store = new CustomStore({\n\tkey: 'id_surat_masuk',\n\tload: function() {\n\t\treturn CrudService.getAll('surat-masuk');\n\t},\n\tinsert: (values) => {\n\t\treturn CrudService.insertData('surat-masuk',values);\n\t},\n\tremove: (key) => {\n\t\treturn CrudService.removeData('surat-masuk',key);\n\t},\n\tupdate: (key, values) => {\n\t\treturn CrudService.updateData('surat-masuk',key,values);\n\t}\n});\n\nexport default {\n  name: 'SuratMasuk',\n  components: {\n    IonButtons,\n    IonContent,\n    IonHeader,\n    IonMenuButton,\n    IonPage,\n    IonTitle,\n    IonToolbar,\n    DxDataGrid,\n    DxColumn,\n    DxPaging,\n    DxEditing,\n    DxPopup,\n    // DxLookup,\n    // DxForm,\n    // DxItem,\n    DxScrolling\n  },\n  methods: {\n    statusColumn_customizeText(cellInfo) {\n        const stext = [\"Draft\",\"Waiting Approval\",\"Approved\",\"Rejected\",\"\"];\n\n        return stext[cellInfo.value];\n    },\n    async openModal() {\n      const modal = await modalController\n        .create({\n          component: Modal,\n          cssClass: 'my-custom-class',\n          componentProps: {\n            title: 'Upload Berkas'\n          },\n          swipeToClose: true,\n        })\n      return modal.present();\n    },\n  },\n  data() {\n    return {\n      dataSource: store,\n    };\n  },\n}\n</script>\n\n<style scoped>\n#container {\n  text-align: center;\n  position: absolute;\n  left: 0;\n  right: 0;\n  top: 50%;\n  transform: translateY(-50%);\n}\n\n#container strong {\n  font-size: 20px;\n  line-height: 26px;\n}\n\n#container p {\n  font-size: 16px;\n  line-height: 22px;\n  color: #8c8c8c;\n  margin: 0;\n}\n\n#container a {\n  text-decoration: none;\n}\n</style>"]}]}