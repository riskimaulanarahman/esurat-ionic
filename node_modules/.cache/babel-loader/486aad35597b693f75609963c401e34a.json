}</ion-card-title>\n          \n        </ion-card-header>\n      </ion-card>\n\n      <ion-card>\n        <ion-card-header>\n          <ion-card-subtitle>Surat Masuk (Belum Dibaca): {{ datadis.nosm }} </ion-card-subtitle>\n          <ion-card-subtitle>Surat Keluar (Belum Dibaca): {{ datadis.nosk }} </ion-card-subtitle>\n        </ion-card-header>\n      </ion-card>\n      <!-- <ion-list v-for=\"item in datadis\" :key=\"item\"> -->\n      <!-- <ion-list>\n        <ion-item>\n          <ion-label>Total Disposisi : {{ datadis.alldisposisi }} </ion-label>\n        </ion-item>\n        <ion-item>\n          <ion-label>Surat Masuk : {{ datadis.nosm }} </ion-label>\n        </ion-item>\n        <ion-item>\n          <ion-label>Surat Keluar : {{ datadis.nosk }} </ion-label>\n        </ion-item>\n      </ion-list> -->\n\n \n    </ion-content>\n  </ion-page>\n</template>\n\n<script lang=\"ts\">\nimport { \n  IonButtons, \n  IonContent, \n  IonHeader, \n  IonMenuButton, \n  IonPage, \n  IonTitle, \n  IonToolbar,\n  IonList,\n  IonCard, IonCardContent, IonCardSubtitle, IonCardTitle, IonIcon, IonItem, IonLabel\n  } from '@ionic/vue';\nimport axios from 'axios';\n\n// const API_URL = 'http://127.0.0.1:8000/api/';\nconst API_URL = 'https://esurat-um.com/api/';\n\n\n// const totdis = axios.get(API_URL + 'dashboard-count').then(function(response){\n//           return response.data.alldisposisi\n//       });\n\n\nexport default {\n  name: 'dashboard',\n  components: {\n    IonButtons,\n    IonContent,\n    IonHeader,\n    IonMenuButton,\n    IonPage,\n    IonTitle,\n    IonToolbar,\n    // IonList,\n    // IonItem,\n    // IonLabel\n    // IonCard, IonCardSubtitle, IonCardTitle,\n  },\n  data() {\n    const datadis = {}\n    return {\n      datadis\n    }\n  },\n  created() {\n    return axios.get(API_URL + 'alluser').then(response => {\n      axios.get(API_URL + 'checknik/'+response.headers.email).then(response => {\n        axios.get(API_URL + 'dashboard-count?nik='+response.data.nik).then(response => {\n        this.datadis = response.data\n        });\n      });\n    });\n  }\n  // provide() {\n  //   return {\n  //     items: this.data\n  //   }\n  // }\n}\n</script>\n\n<style scoped>\n#container {\n  text-align: center;\n  position: absolute;\n  left: 0;\n  right: 0;\n  top: 50%;\n  transform: translateY(-50%);\n}\n\n#container strong {\n  font-size: 20px;\n  line-height: 26px;\n}\n\n#container p {\n  font-size: 16px;\n  line-height: 22px;\n  color: #8c8c8c;\n  margin: 0;\n}\n\n#container a {\n  text-decoration: none;\n}\n</style>"],"sourceRoot":""}]}