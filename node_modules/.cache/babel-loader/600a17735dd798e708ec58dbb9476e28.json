t\t\tfooter: '<a href>Why do I have this issue?</a>'\n\t\t\t})\n\t\t})\n\t}\n});\n\nexport default {\n  name: 'SuratMasuk',\n  components: {\n    IonButtons,\n    IonContent,\n    IonHeader,\n    IonMenuButton,\n    IonPage,\n    IonTitle,\n    IonToolbar,\n    DxDataGrid,\n    DxColumn,\n    DxPaging,\n    DxEditing,\n    DxPopup,\n    // DxLookup,\n    DxForm,\n    DxItem,\n    DxScrolling\n  },\n  data() {\n    return {\n      dataSource: store,\n    //   states: states\n      // columns: ['CompanyName', 'City', 'State', 'Phone', 'Fax']\n    };\n  }\n}\n</script>\n\n<style scoped>\n#container {\n  text-align: center;\n  position: absolute;\n  left: 0;\n  right: 0;\n  top: 50%;\n  transform: translateY(-50%);\n}\n\n#container strong {\n  font-size: 20px;\n  line-height: 26px;\n}\n\n#container p {\n  font-size: 16px;\n  line-height: 22px;\n  color: #8c8c8c;\n  margin: 0;\n}\n\n#container a {\n  text-decoration: none;\n}\n</style>"],"sourceRoot":""}]}